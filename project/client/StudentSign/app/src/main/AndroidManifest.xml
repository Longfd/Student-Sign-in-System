<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.lxj.studentsign">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".ui.TeacherMainActivity">
        </activity>

        <activity android:name=".ui.RegisterActivity"
            >
        </activity>

        <activity android:name=".ui.ClassListActivity"
            />

        <activity android:name=".ui.StudentListActivity"
            />

        <activity android:name=".ui.ActiveListActivity"
            />

        <activity android:name=".ui.CreateActiveActivity"
            />

        <activity android:name=".ui.SignListActivity"
            />

        <activity android:name=".ui.StudentMainActivity">

        </activity>

        <activity android:name=".ui.SettingActivity"/>

    </application>

</manifest>